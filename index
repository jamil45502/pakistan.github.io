<script>
    function utmParamGenerator(shortName, longName, isChar2, isPersistent) {
        var sessionStorage;
        var storedSession = null;
        var regExMatch = isChar2 ? '=(\\w{2})' : window.EDM.Venom.UTM_REGEX_MATCH;
        var longUtm = longName + regExMatch;
        var longUtmReg = new RegExp(longUtm);
        var shortUtm = shortName + regExMatch;
        var shortUtmReg = new RegExp(shortUtm);
        var UTM_STORAGE_KEY = shortName;
        try {
            sessionStorage = window.sessionStorage;
            storedSession = sessionStorage && sessionStorage.getItem(UTM_STORAGE_KEY);
        } catch (e) {
            sessionStorage = null;
        }
        var EDMUNDS_DOMAIN = '.edmunds.com';
        var state = window.__PRELOADED_STATE__ || {};
        var searchMatch = window.location.search.match(longUtmReg);
        var cookieMatch = document.cookie.match(shortUtmReg);
        var storedCookie = cookieMatch ? cookieMatch[1] : null;
        var setUtmCookie = function(value) {
            document.cookie = `${UTM_STORAGE_KEY}=${value}; path=/; domain=${EDMUNDS_DOMAIN}; sameSite=Lax`;
        }
        var removeUtmCookie = function() {
            document.cookie = `${UTM_STORAGE_KEY}=; expires=Thu, 01 Jan 1970 00:00:01 GMT; path=/; domain=${EDMUNDS_DOMAIN};`;
        }
        var value;
        if (isPersistent) {
            if (searchMatch) {
                value = searchMatch[1];
                sessionStorage && sessionStorage.setItem(UTM_STORAGE_KEY, value);
                setUtmCookie(value);
            } else if (document.referrer.includes(`forums ${EDMUNDS_DOMAIN}`) && storedCookie) {
                sessionStorage && sessionStorage.setItem(UTM_STORAGE_KEY, storedCookie);
            } else if (storedSession) {
                setUtmCookie(storedSession);
            } else {
                removeUtmCookie();
            }
            state.pageContext.ads[shortName] = sessionStorage && sessionStorage.getItem(UTM_STORAGE_KEY) || '';
        }
    }
    var utmParams = window.EDM.Venom.UTM_PARAMS;
    for (var i = 0; i < utmParams.length; i++) {
        var shortName = utmParams[i].shortName;
        var longName = utmParams[i].longName;
        var isChar2 = utmParams[i].isChar2;
        var isPersistent = utmParams[i].isPersistent;
        utmParamGenerator(shortName, longName, isChar2, isPersistent);
    }
</script>
